<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.fanxuankai.boot</groupId>
        <artifactId>fxk-boot-mqbroker</artifactId>
        <version>${revision}</version>
    </parent>
    <artifactId>fxk-boot-mqbroker-autoconfigure</artifactId>
    <packaging>pom</packaging>
    <name>FXK Boot MQ Broker Autoconfigure</name>
    <description>FXK Boot MQ Broker Autoconfigure</description>
    <modules>
        <module>fxk-boot-mqbroker-kafka-autoconfigure</module>
        <module>fxk-boot-mqbroker-rabbit-autoconfigure</module>
        <module>fxk-boot-mqbroker-rocket-autoconfigure</module>
